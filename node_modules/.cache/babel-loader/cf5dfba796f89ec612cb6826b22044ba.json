CA,QAAA,MAAA,EAAA;AACA,UAAA,OAAA,EAAA,GADA;AAEA,UAAA,QAAA,EAAA,KAAA,UAAA,CAAA,cAFA;AAGA,UAAA,UAAA,EAAA,KAAA,UAAA,CAAA,MAHA;AAIA,UAAA,MAAA,EAAA,KAAA;AAJA;AADA,OADA,EASA,IATA,CASA,UAAA,QAAA,EAAA;AACA,YAAA,SAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,SAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,mCAAA,EAAA,SAAA;AACA,QAAA,KAAA,CAAA,UAAA,CAAA,OAAA,GAAA,SAAA,CAAA,UAAA;AACA,QAAA,KAAA,CAAA,UAAA,CAAA,MAAA,GAAA,SAAA,CAAA,WAAA;AACA,QAAA,KAAA,CAAA,UAAA,CAAA,cAAA,GAAA,SAAA,CAAA,QAAA;AACA,OAhBA,EAiBA,KAjBA,CAiBA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,kBAAA,EAAA,KAAA;AACA,OAnBA;AAoBA,KA1BA;AA2BA,IAAA,YA3BA,0BA2BA,CAEA;AA7BA,GAnBA;AAkDA,EAAA,OAlDA,qBAkDA;AACA;AACA,SAAA,OAAA;AACA;AArDA,CAAA","sourcesContent":["<template>\r\n<div>\r\n\r\n\r\n    <v-data-table\r\n        :headers=\"headers\"\r\n        :items=\"dias\"\r\n        :hide-default-footer=\"true\"\r\n        class=\"elevation-1\"\r\n    >\r\n        <template v-slot:top>\r\n            <v-toolbar flat>\r\n                <!-- <v-toolbar-title>BUSCADOR</v-toolbar-title> -->\r\n               <!--  <v-text-field\r\n                    v-model=\"search\"\r\n                    label=\"Buscar\"\r\n                    class=\"mx-4 my-4\"\r\n                ></v-text-field> -->\r\n <!--                <v-btn color=\"primary\" @click=\"aplicarBusqueda\"\r\n                    >Buscar</v-btn\r\n                > -->\r\n\r\n                <v-divider class=\"mx-4\" inset vertical></v-divider>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"success\" @click=\"nuevoHorario\">\r\n                    Agregar Horario\r\n                </v-btn>\r\n            </v-toolbar>\r\n        </template>\r\n        <template v-slot:item.actions=\"{ item }\">\r\n            <p @click=\"verItem(item.key)\">Ver</p>\r\n            <v-icon small class=\"mr-2\" @click=\"editItem(item.key)\">\r\n                mdi-pencil\r\n            </v-icon>\r\n            <v-icon small @click=\"deleteItem(item)\">\r\n                mdi-delete\r\n            </v-icon>\r\n        </template>\r\n    </v-data-table>\r\n    <!---  PAGINACION --->\r\n    <div class=\"text-xs-center pt-2 md4 lg4\">\r\n        <!-- I removed v-model and added @input and :value -->\r\n        <div class=\"form-group col-4\">\r\n            <label> Resultados por pagina</label>\r\n            <select\r\n                class=\"form-control\"\r\n                style=\"width: 50px; border: 1px solid black\"\r\n                v-model=\"paginacion.filasPorPagina\"\r\n                @change=\"filasPorPaginaHandler()\"\r\n            >\r\n                <option value=\"5\">5</option>\r\n                <option value=\"10\">10</option>\r\n                <option value=\"15\">15</option>\r\n                <option value=\"20\">20</option>\r\n                <option value=\"25\">25</option>\r\n            </select>\r\n        </div>\r\n\r\n        <v-pagination\r\n            @input=\"cambioDePaginaHandler\"\r\n            :value=\"paginacion.pagina\"\r\n            :length=\"paginacion.paginas\"\r\n        ></v-pagination>\r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\nexport default{\r\n    name:'Horario',\r\n    data: () => ({\r\n        headers: [\r\n            {\r\n                text: \"Dia\",\r\n                align: \"start\",\r\n                sortable: true,\r\n                value: \"dia\",\r\n            },\r\n            { text: \"Hora inicio\", value: \"horaInicio\" },\r\n            { text: \"Hora fin\", value: \"horaFin\" },\r\n\r\n            { text: \"Actions\", value: \"actions\", sortable: false },\r\n        ],\r\n        dias: [],\r\n        paginacion: {},\r\n        url: \"https://clinicmanagementwebservice20211116223102.azurewebsites.net\",\r\n    }),\r\n    methods: {\r\n        aplicarBusqueda() {\r\n            console.log(\"busqueda..\", this.search);\r\n            this.listDia();\r\n        },\r\n        listDia() {\r\n            axios\r\n                .get(this.url + \"/api/v1/Dia\", {\r\n                    params: {\r\n                        version: \"1\",\r\n                        PageSize: this.paginacion.filasPorPagina,\r\n                        PageNumber: this.paginacion.pagina,\r\n                        Search: this.search,\r\n                    },\r\n                })\r\n                .then((response) => {\r\n                    let respuesta = response.data;\r\n                    this.dias = respuesta.rows;\r\n                    console.log(\"Esta es la respuesta del servidor\", respuesta);\r\n                    this.paginacion.paginas = respuesta.totalPages;\r\n                    this.paginacion.pagina = respuesta.currentPage;\r\n                    this.paginacion.filasPorPagina = respuesta.pageSize;\r\n                })\r\n                .catch((error) => {\r\n                    console.log(\"Ocurrio un error\", error);\r\n                });\r\n        },\r\n        nuevoHorario(){\r\n\r\n        }\r\n    },\r\n    created() {\r\n        //this.initialize();\r\n        this.listDia();\r\n    }\r\n}\r\n</script>"],"sourceRoot":"src/components"}]}