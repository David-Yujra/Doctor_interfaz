  <v-btn\r\n                                color=\"purple darken-1\"\r\n                                text\r\n                                v-for=\"dia in dias\"\r\n                                :key=\"dia\"\r\n                                @click=\"abrirModalHoras(dia)\"\r\n                            >\r\n                                {{ dia }}\r\n                            </v-btn>\r\n                        </div>\r\n                    </v-row>\r\n                    <v-row>\r\n                        <v-col cols=\"12\">\r\n                            <v-list-item two-line>\r\n                                <v-list-item-content v-for=\"horario in horariosDoctor\" :key=\"horario.diaDoctorId\">\r\n                                    <v-list-item-title>{{horario.diaSemana}}</v-list-item-title>\r\n                                    <v-list-item-subtitle>{{horario.horaInicio}} - {{horario.horaFin}}</v-list-item-subtitle>\r\n                                </v-list-item-content>\r\n                            </v-list-item>\r\n                        </v-col>\r\n                    </v-row>\r\n                    <v-row>\r\n                        <v-col cols=\"6\">\r\n                            <v-btn\r\n                                color=\"red darken-1\"\r\n                                text\r\n                                @click=\"modalDias = true\"\r\n                            >\r\n                                Editar\r\n                            </v-btn>\r\n                            <v-btn\r\n                                color=\"red darken-1\"\r\n                                text\r\n                                @click=\"NuevoHorario = true\"\r\n                            >\r\n                                Nuevo Horario\r\n                            </v-btn>\r\n                            \r\n                        </v-col>\r\n                    </v-row>\r\n\r\n                    <v-card-actions>\r\n                        <v-spacer></v-spacer>\r\n                        <v-btn\r\n                            color=\"blue darken-1\"\r\n                            text\r\n                            @click=\"nuevoDoctor = false\"\r\n                        >\r\n                            Cancelar\r\n                        </v-btn>\r\n                        <v-btn\r\n                            v-show=\"editar\"\r\n                            color=\"blue darken-1\"\r\n                            text\r\n                            @click=\"actualizar\"\r\n                        >\r\n                            actualizar\r\n                        </v-btn>\r\n                        <v-btn\r\n                            v-show=\"!editar\"\r\n                            color=\"blue darken-1\"\r\n                            text\r\n                            @click=\"save\"\r\n                        >\r\n                            Guardar\r\n                        </v-btn>\r\n                    </v-card-actions>\r\n\r\n                    <v-row>\r\n                        <v-col cols=\"12\">\r\n                            <v-btn color=\"blue darken-1\" text @click=\"save\">\r\n                            </v-btn>\r\n                        </v-col>\r\n                    </v-row>\r\n                </v-card-text>\r\n            </v-card>\r\n        </v-dialog>\r\n        <!---  MODAL CONFIRMAR BORRAR DOCTOR --->\r\n        <v-dialog\r\n            transition=\"dialog-bottom-transition\"\r\n            max-width=\"1000\"\r\n            v-model=\"modalBorrar\"\r\n        >\r\n            <v-card>\r\n                <v-card-title> Esta seguro de borrar al Doctor? </v-card-title>\r\n                <v-card-text>\r\n                    <v-card-actions>\r\n                        <v-spacer></v-spacer>\r\n                        <v-btn\r\n                            color=\"blue darken-1\"\r\n                            text\r\n                            @click=\"modalBorrar = false\"\r\n                        >\r\n                            Cancelar\r\n                        </v-btn>\r\n                        <v-btn\r\n                            color=\"blue darken-1\"\r\n                            text\r\n                            @click=\"deleteItemConfirm\"\r\n                        >\r\n                            Confirmar\r\n                        </v-btn>\r\n                    </v-card-actions>\r\n                </v-card-text>\r\n            </v-card>\r\n        </v-dialog>\r\n        <!---  MODAL VER DOCTOR --->\r\n        <v-dialog\r\n            transition=\"dialog-bottom-transition\"\r\n            max-width=\"1000\"\r\n            v-model=\"verDoctor\"\r\n        >\r\n            <v-card>\r\n                <v-card-title> Agregar nuevo doctor </v-card-title>\r\n                <v-card-text>\r\n                    <v-row>\r\n                        <v-col cols=\"6\">\r\n                            <p>Nombre completo</p>\r\n                            <h4>{{ editedItem.nombreCompleto }}</h4>\r\n                        </v-col>\r\n                        <v-col cols=\"6\">\r\n                            <p>Categoria</p>\r\n                            <h4>{{ editedItem.categoria }}</h4>\r\n                        </v-col>\r\n                        <v-col cols=\"6\">\r\n                            <p>Genero</p>\r\n                            <h4>{{ editedItem.genero }}</h4>\r\n                        </v-col>\r\n                        <v-col cols=\"6\">\r\n                            <p>telefonoFijo</p>\r\n                            <h4>{{ editedItem.telefonoFijo }}</h4>\r\n                        </v-col>\r\n                        <v-col cols=\"6\">\r\n                            <p>fechaDeNacimiento</p>\r\n                            <h4>{{ editedItem.fechaDeNacimiento }}</h4>\r\n                        </v-col>\r\n                        <v-col cols=\"6\">\r\n                            <p>numeroCelular</p>\r\n                            <h4>{{ editedItem.numeroCelular }}</h4>\r\n                        </v-col>\r\n                        <v-col cols=\"6\">\r\n                            <p>email</p>\r\n                            <h4>{{ editedItem.email }}</h4>\r\n                        </v-col>\r\n                        <v-col cols=\"6\">\r\n                            <p>especialidadKey</p>\r\n                            <h4>{{ editedItem.especialidadKey }}</h4>\r\n                        </v-col>\r\n                        <v-col cols=\"6\">\r\n                            <p>estado</p>\r\n                            <h4>{{ editedItem.estado }}</h4>\r\n                        </v-col>\r\n                    </v-row>\r\n                    <v-row>\r\n                        <v-col cols=\"3\">\r\n                            <v-btn color=\"purple darken-1\" text @click=\"save\">\r\n                                `Lunes `\r\n                            </v-btn>\r\n                        </v-col>\r\n                        <v-col cols=\"3\">\r\n                            <v-btn color=\"purple darken-1\" text @click=\"save\">\r\n                                `Domingo `\r\n                            </v-btn>\r\n                        </v-col>\r\n                    </v-row>\r\n                    <v-card-actions>\r\n                        <v-spacer></v-spacer>\r\n                        <v-btn\r\n                            color=\"blue darken-1\"\r\n                            text\r\n                            @click=\"verDoctor = false\"\r\n                        >\r\n                            Cancel\r\n                        </v-btn>\r\n                    </v-card-actions>\r\n                </v-card-text>\r\n            </v-card>\r\n        </v-dialog>\r\n        <!---  MODAL CONFIRMAR DIAS --->\r\n        <v-dialog\r\n            transition=\"dialog-bottom-transition\"\r\n            max-width=\"500\"\r\n            v-model=\"modalDias\"\r\n        >\r\n