v-checkbox\r\n                                    v-model=\"dias\"\r\n                                    label=\"Domingo\"\r\n                                    value=\"Domingo\"\r\n                                ></v-checkbox>\r\n                            </div>\r\n                        </div>\r\n                    </v-container>\r\n                    <v-card-actions>\r\n                        <v-spacer></v-spacer>\r\n                        <v-btn\r\n                            color=\"blue darken-1\"\r\n                            text\r\n                            @click=\"modalDias = false\"\r\n                        >\r\n                            Cancelar\r\n                        </v-btn>\r\n                        <v-btn\r\n                            color=\"blue darken-1\"\r\n                            text\r\n                            @click=\"confirmarDias\"\r\n                        >\r\n                            Confirmar\r\n                        </v-btn>\r\n                    </v-card-actions>\r\n                </v-card-text>\r\n            </v-card>\r\n        </v-dialog>\r\n        <!---  MODAL CONFIRMAR HORAS --->\r\n        <v-dialog\r\n            transition=\"dialog-bottom-transition\"\r\n            max-width=\"1000\"\r\n            v-model=\"modalHoras\"\r\n        >\r\n            <v-card>\r\n                <v-card-title>\r\n                    Ingrese el horario para el dia {{ diaElegido }}\r\n                </v-card-title>\r\n                <v-card-text>\r\n                    <div>\r\n                        <v-btn\r\n                            color=\"success\"\r\n                            v-show=\"!crearIntervalo\"\r\n                            @click=\"crearIntervalo = true\"\r\n                            >Nuevo Intervalo</v-btn\r\n                        >\r\n                        <v-btn\r\n                            color=\"primary\"\r\n                            v-show=\"crearIntervalo\"\r\n      